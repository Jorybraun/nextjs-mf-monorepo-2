{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "build:remotes": {
      "outputs": []
    },
    "build:host": {
      "dependsOn": ["build:remotes"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "dev:remotes": {},
    "dev:host": {
      "dependsOn": ["dev:remotes"]
    },
    "lint": {},
    "test": {}
  }
}